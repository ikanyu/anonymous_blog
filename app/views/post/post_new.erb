<div class="container">
	<h2>Create New Post</h4>
	<form action="/posts/create" method="POST">
		<h3>Title</h3>
		<input type="text" name="post[title]">
		<h3>Description</h3>
		<input type="text" name="post[content]">
		<h4>Tags</h4>
		<input id="post_tags" name="tags">
		<input type="submit" value="Submit">
	</form>
</div>

